{
  "name": "cross-market-etl-pipeline",
  "version": "1.0.0",
  "description": "ETL pipeline for cross-market financial data (TradFi + Crypto)",
  "main": "index.js",
  "scripts": {
    "start": "node src/etl/dukascopy_importer.js",
    "crypto": "python src/etl/crypto_importer.py",
    "import:all": "node run_all_importers.js",
    "import:tradfi": "node run_all_importers.js --tradfi-only",
    "import:crypto": "node run_all_importers.js --crypto-only",
    "import:m5": "TIMEFRAMES=m5 node run_all_importers.js",
    "import:h1": "TIMEFRAMES=h1 node run_all_importers.js",
    "import:m5h1": "TIMEFRAMES=m5,h1 node run_all_importers.js",
    "db:test": "node -e \"require('./src/config/database').testConnection()\"",
    "db:import": "node scripts/import_to_database.js",
    "db:stats": "node -e \"const mgr = require('./src/database/symbol_manager'); new mgr().metadataManager.getAllSymbolMetadata().then(console.log)\"",
    "metadata:generate": "node -e \"const mgr = require('./src/database/symbol_metadata_manager'); new mgr().generateAllMetadata()\"",
    "schedule:check": "node src/services/scheduled_updater.js --check",
    "schedule:run": "node src/services/scheduled_updater.js --run",
    "scheduler:start": "node src/services/metadata_scheduler.js",
    "scheduler:test": "node src/services/metadata_scheduler.js --test-metadata",
    "scheduler:once": "node src/services/metadata_scheduler.js --run-once",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "etl",
    "financial-data",
    "dukascopy",
    "crypto",
    "trading"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "dukascopy-node": "^1.43.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.12.0"
  }
}
